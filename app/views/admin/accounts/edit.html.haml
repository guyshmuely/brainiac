%fieldset.field
  %legend
    Edit account
  =link_to 'back', admin_accounts_path , :class=>'button' ,:id=>'back'
  -form_for ['admin',@account] do |f|
    %p.inputrow
      =f.label 'Email:'
      %br
      =f.text_field :email
    %p.inputrow
      =f.label 'Password:'
      %br
      =f.password_field :password
    %p=f.submit

